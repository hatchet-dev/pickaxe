{
  "watch": ["src"],
  "ext": "ts,json",
  "ignore": [
    "src/**/*.test.ts",
    "src/**/*.spec.ts",
    "dist/**/*",
    "node_modules/**/*"
  ],
  "exec": "ts-node -r tsconfig-paths/register src/main.ts",
  "env": {
    "NODE_ENV": "development"
  },
  "delay": "1000",
  "verbose": true,
  "restartable": "rs"
}